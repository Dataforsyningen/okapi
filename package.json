{
  "name": "webpack-library-starter",
  "version": "1.0.2",
  "description": "Produce universal library with webpack and es6",
  "main": "lib/okapi.js",
  "scripts": {
    "build": "webpack --progress --colors --env build && webpack --env build --config=webpack.config.examples.js",
    "build:new": "webpack --progress --colors --env build --config=webpack.config.examples.js",
    "examples": "webpack --env build --config=webpack.config.examples.js",
    "dev": "webpack --progress --colors --watch --env dev",
    "dev:new": "cross-env OKAPI_ENV=production OKAPI_MINIFY=false webpack --progress --colors --config=webpack.config.dev.js",
    "stats": "webpack --profile --json > stats.json",
    "analyze": "webpack-bundle-analyzer stats.json",
    "test": "mocha --require babel-register --colors ./test/*.spec.js",
    "test:watch": "mocha --require babel-register --colors -w ./test/*.spec.js",
    "test:cover": "cross-env NODE_ENV=test nyc mocha --require babel-register --colors test/*.js",
    "repl": "node -i -e \"$(< ./lib/webpack-library-starter.js)\"",
    "lint": "./node_modules/.bin/eslint src --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/krasimir/webpack-library-starter.git"
  },
  "keywords": [
    "webpack",
    "es6",
    "starter",
    "library",
    "universal",
    "umd",
    "commonjs"
  ],
  "author": "SDFE",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/krasimir/webpack-library-starter/issues"
  },
  "homepage": "https://github.com/krasimir/webpack-library-starter",
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.10.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.16",
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/preset-env": "^7.12.16",
    "@babel/register": "^7.12.13",
    "babel-loader": "^8.2.2",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-plugin-istanbul": "^5.2.0",
    "base64-inline-loader": "^1.1.1",
    "chai": "^4.3.0",
    "cross-env": "^5.2.1",
    "css-loader": "^1.0.1",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.2.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "11.11.0",
    "jsdom-global": "3.0.2",
    "mini-css-extract-plugin": "^0.5.0",
    "mocha": "^8.3.0",
    "nyc": "^14.1.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "proj4": "^2.7.0",
    "stylus": "^0.54.8",
    "stylus-loader": "^3.0.2",
    "terser-webpack-plugin": "^4.2.3",
    "uglifyjs-webpack-plugin": "^2.0.0",
    "webpack": "^4.46.0",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
    "webpack-subresource-integrity": "^1.5.2",
    "yargs": "^16.2.0"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "dependencies": {
    "array.prototype.find": "^2.1.1",
    "axios": "^0.21.1",
    "dotenv": "^7.0.0",
    "ejs-loader": "^0.3.7",
    "get-sri": "^1.0.2",
    "html-loader": "^0.5.5",
    "ol": "^6.5.0",
    "promise-polyfill": "^8.2.0",
    "style-loader": "^0.23.1"
  }
}
